
        local Library =
            loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("NEW VIDEO OUT FOR UPDATED HC6 GUI", "DarkTheme")

        -- MAIN
        local Main = Window:NewTab("Main")
        local MainSection = Main:NewSection("Main")

MainSection:NewButton("GET VIDEO LINK", "PASTE LINK TO GOOGLE", function()
local CoreGui = game:GetService("StarterGui") -- Variable of StarterGui
	CoreGui:SetCore("SendNotification", {
		-- Customizable
		Title = "Paste To Google",
		Text = "Copied To Clipboard",
		Duration = 7, -- Set the duration to how much you want this to stay
		-- More code in part 2
	})
    setclipboard("https://www.youtube.com/watch?v=30gMQj-Pgwc")
	toclipboard("https://www.youtube.com/watch?v=30gMQj-Pgwc")
            end)
        
